<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/home_dh_qmx_1"
    android:paddingTop="80dp"
    android:orientation="vertical">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/jikai_new"
        android:layout_gravity="right"
        android:layout_marginRight="20dp"
        />
    <ImageView
        android:id="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/home_dh_qmx_2" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/home_dh_qmx_3" />

    <ImageView
        android:id="@+id/secend"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/home_dh_qmx_4" />

    <ImageView
        android:id="@+id/three"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/home_dh_qmx_5" />
</FrameLayout>
