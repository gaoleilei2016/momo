<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gdswww.com.momo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".base.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".activity.me.MineActivity"
            android:theme="@style/MineTheme"
            />
        <activity android:name=".activity.me.EditDataActivity"/>
        <activity android:name=".activity.me.SetUpActivity" />
        <activity android:name=".activity.home.TabIndexActivity" />
        <activity android:name=".activity.me.RechargeActivity" />
        <activity android:name=".activity.home.RankingActivity" />
        <activity android:name=".activity.me.MessageActivity" />
        <activity android:name=".activity.home.SearchAnchorActivity" />
        <activity android:name=".activity.home.HomePageActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".activity.me.ListViewDeleteItemActivity"/>
        <activity android:name=".activity.me.TopActivity"/>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>